{
  "name": "user-feedback",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn build && npx serve -s ./build -p 3000",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint ./ --config=.eslintrc.prod.json --ext=js,ts,tsx",
    "prettify": "prettier . --write",
    "analyze": "source-map-explorer 'build/static/js/**/*.js' --gzip",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test -- --watchAll=false & yarn cy:run",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "**/*.{js,ts,tsx}": [
      "eslint ./ --config=.eslintrc.prod.json --fix"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/history": "^4.7.7",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "dayjs": "^1.8.32",
    "emotion": "^10.0.27",
    "frctl": "^2.0.0",
    "history": "^5.0.0",
    "mapbox-gl": "^1.11.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-mapbox-gl": "^4.8.6"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cypress": "^4.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.7"
  }
}
