{
  "name": "user-feedback",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --config=.eslintrc.prod.json --ext=js,ts,tsx",
    "prettify": "prettier \"{src,cypress}/**/*.{js,ts,tsx,json,md}\" --write",
    "analyze": "source-map-explorer 'build/static/js/**/*.js' --gzip",
    "sg:run": "styleguidist server",
    "sg:build": "styleguidist build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.{js,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "{src}/**/*.{js,ts,tsx}": [
      "eslint ./ --config=.eslintrc.prod.json --fix"
    ]
  },
  "dependencies": {
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "frctl": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-docgen-typescript": "^1.20.1",
    "react-scripts": "^3.4.1",
    "react-styleguidist": "^11.0.8",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.7"
  }
}
